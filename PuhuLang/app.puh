void main(){
	double avgTime = 0;
	for(int i = 0; i < 10; i = i + 1){
		double startTime = clock();
		double x = fibb(21.0 + i);
		double elapsed = clock() - startTime;
		println(x);
		print("Elapsed time: ");
		println(elapsed);
		avgTime = avgTime + elapsed;
	}
	println(avgTime/10);
}

double fact(double n){
	if(n > 1.0) {
		return n * fact(n-1.0);
    }
    else 
		return 1.0;
}

double fibb(double n){
	if(n > 1.0) {
		return fibb(n-1.0) + fibb(n-2.0);
    }
    else 
		return 1.0;
}