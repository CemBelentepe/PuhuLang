void main(){
	for(int i = 0; i < 10; i = i + 1){
		double start = clock();
		double x = fibb(30.0);
		double end = clock();
		double elapsed = end - start;
		printlnDouble(x);
		printlnDouble(elapsed);
	}
}

double fibb(double n){
	if(n > 1.0)
		return fibb(n - 1.0) + fibb(n - 2.0);
	else
		return 1.0;
}