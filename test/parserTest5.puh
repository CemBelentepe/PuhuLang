void main()
{
    int n = 10;
    for(int i = 0; i < n; i = i + 1){
        show(i);
        if(i != n-1)
            print(", ");
    }

    print("\nHi\n");
    print("3 + 9 = ");
    show(3+9);
    print("\n");
    max(16,17);
    int k = max(20,10);
    show(k);
}

int max(int a, int b)
{
    if(a > b) return a;
    return b;
}